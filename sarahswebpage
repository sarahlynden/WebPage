        <script>
			var message = document.getElementById("message");
			message.onmouseover = function() {
				message.innerHTML ="Please look around!";
				};
			message.onmouseout = function() {
				message.innerHTML ="This web page contains all of the projects I have built so far!";
				};
		
			var currentImage = 0;
			var myImages = ["Screenshot (1).png","Screenshot (3).png","Screenshot (4).png","Screenshot (5).png","Screenshot (6).png"];

			document.getElementById("button").onclick = function() {
				currentImage += 1;
				currentImage %= myImages.length;
				document.getElementById("images").src = myImages[currentImage];
				};
			$(document).ready(function(){
				$('#hello').mouseenter(function(){
					$('#hello').hide();
				});
				$('#hello').mouseout(function(){
					$('#hello').show();
				});
			});
		</script>
